# Development Environment Variables (for devcontainer)

# Mock Data Configuration
NEXT_PUBLIC_USE_MOCK_DATA=true

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Sports.Live

# Feature Flags (all disabled for mock data mode)
NEXT_PUBLIC_ENABLE_AUTH=false
NEXT_PUBLIC_ENABLE_REALTIME=false
NEXT_PUBLIC_ENABLE_RATINGS=false

# Supabase (not needed for mock data)
# NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Optional: Upstash Redis (not needed for mock data)
# UPSTASH_REDIS_REST_URL=your_redis_rest_url
# UPSTASH_REDIS_REST_TOKEN=your_redis_rest_token

# Optional: NBA API (not needed for mock data)
# NBA_API_KEY=your_nba_api_key
